<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valid_Check_Out</fullName>
    <active>true</active>
    <errorConditionFormula>AND (Check_In__c &gt; Check_Out__c)</errorConditionFormula>
    <errorDisplayField>Check_Out__c</errorDisplayField>
    <errorMessage>You cannot select a date that is earlier than the check-in date.</errorMessage>
</ValidationRule>
